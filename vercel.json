{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "PORT": "5005",
        "ORIGIN": "http://localhost:5173",
        "TOKEN_SECRET": "secreto"
    }
}
